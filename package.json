{
  "name": "daynight-clock",
  "version": "0.0.2",
  "private": false,
  "type": "module",
  "module": "./lib/esm/index.js",
  "main": "./lib/cjs/index.js",
  "license": "MIT",
  "files": [
    "lib/"
  ],
  "browser": "./browser/specific/main.js",
  "description": "A clock that expresses day and night as diagonally divided areas.",
  "keywords": [
    "react",
    "watch",
    "clock",
    "divided",
    "day",
    "night"
  ],
  "author": {
    "name": "YounsangNa",
    "email": "nayounsang722@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nayounsang/daynight-clock.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "copy": "copyfiles src/components/style/*.css lib/esm && copyfiles src/components/style/*.css lib/cjs",
    "clean": "rimraf lib",
    "tsc-build": "npm run clean && tsc -p tsconfig-cjs.json && tsc -p tsconfig-esm.json && npm run copy"
    
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "timezone-support": "^3.1.0",
    "usehooks-ts": "^2.9.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "typescript": "^5.2.2",
    "typescript-plugin-css-modules": "^5.0.2",
    "vite": "^5.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
